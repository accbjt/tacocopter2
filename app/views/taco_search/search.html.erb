
<div class="tacos-container row">
  <div class='row'> 
    <div class='title'> Stores Matching Your Requirements: </div>
    <table>
      <thead>
        <tr>
          <th>Store</th>
          <th>City</th>
        </tr>
      </thead>
      <tbody>
        <% @stores.each do |store| %>
        <tr>
          <td><%= store.name %></td>
          <td><%= store.city_name %></td>
        </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>